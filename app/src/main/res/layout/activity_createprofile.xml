<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-25dp"
        tools:layout_editor_absoluteY="16dp">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="36dp"
            app:srcCompat="@drawable/arrow_icon"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/Profile"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="111dp"
            android:layout_marginTop="36dp"
            android:text="Profile"
            android:textColor="@color/Watermelon"
            android:textSize="25sp"
            tools:ignore="TextSizeCheck" />

        <ImageButton
            android:id="@+id/edit"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="270dp"
            android:layout_marginTop="38dp"
            android:layout_marginEnd="75dp"
            android:background="@drawable/button_style5"
            android:backgroundTint="#F8E9EB"
            app:srcCompat="@drawable/drawable_edit1"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/vert"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="330dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="19dp"
            android:background="@drawable/button_style5"
            android:backgroundTint="#F8E9EB"
            app:srcCompat="@drawable/drawable_vert"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/profilepic"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:layout_marginStart="158dp"
            android:layout_marginTop="113dp"
            app:srcCompat="@drawable/img" />

        <View
            android:id="@+id/divider1"
            android:layout_width="370dp"
            android:layout_height="2dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="282dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imageButton1"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="290dp"
            android:background="@drawable/button_style6"
            app:srcCompat="@drawable/drawable_editprofile"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="123dp"
            android:layout_marginTop="290dp"
            android:text="Edit Profile"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="350dp"
            android:layout_marginTop="291dp"
            android:layout_marginEnd="25dp"
            app:srcCompat="@drawable/drawable_arrowright"
            tools:ignore="ImageContrastCheck" />

        <View
            android:id="@+id/divider2"
            android:layout_width="370dp"
            android:layout_height="2dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="351dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="359dp"
            android:background="@drawable/button_style6"
            app:srcCompat="@drawable/drawable_notification"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="123dp"
            android:layout_marginTop="359dp"
            android:text="Notification"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="350dp"
            android:layout_marginTop="360dp"
            android:layout_marginEnd="25dp"
            app:srcCompat="@drawable/drawable_arrowright"
            tools:ignore="ImageContrastCheck" />

        <View
            android:id="@+id/divider3"
            android:layout_width="370dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="420dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="428dp"
            android:background="@drawable/button_style6"
            app:srcCompat="@drawable/drawable_security"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="123dp"
            android:layout_marginTop="428dp"
            android:text="Security"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="350dp"
            android:layout_marginTop="429dp"
            android:layout_marginEnd="25dp"
            app:srcCompat="@drawable/drawable_arrowright"
            tools:ignore="ImageContrastCheck" />

        <View
            android:id="@+id/divider4"
            android:layout_width="370dp"
            android:layout_height="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="489dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="497dp"
            android:background="@drawable/button_style6"
            app:srcCompat="@drawable/drawable_darkmode"
            tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="123dp"
            android:layout_marginTop="497dp"
            android:text="Dark Mode"
            android:textSize="20sp" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_below="@+id/divider4"
            android:layout_marginStart="350dp"
            android:layout_marginEnd="25dp"

            tools:ignore="UseSwitchCompatOrMaterialXml,TouchTargetSizeCheck" />

        <View
            android:id="@+id/divider5"
            android:layout_width="370dp"
            android:layout_height="2dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="555dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imagebutton5"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
          android:layout_marginTop="570dp"
            android:background="@drawable/button_style6"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/drawable_friends"
            tools:ignore="MissingConstraints,ImageContrastCheck,SpeakableTextPresentCheck"
            tools:layout_editor_absoluteY="546dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="124dp"
            android:layout_marginTop="570dp"
            android:text="Invite Friends"
            android:textSize="20dp"
            tools:ignore="MissingConstraints,TextSizeCheck,VisualLintOverlap"
            tools:layout_editor_absoluteX="125dp"
            tools:layout_editor_absoluteY="546dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="25dp"
            android:layout_below="@+id/divider5"
            android:layout_marginStart="350dp"
            app:srcCompat="@drawable/drawable_arrowright"
            tools:ignore="MissingConstraints,ImageContrastCheck"
            tools:layout_editor_absoluteX="333dp"
            tools:layout_editor_absoluteY="427dp" />


        <View
            android:id="@+id/divider6"
            android:layout_width="370dp"
            android:layout_height="2dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="620dp"
            android:layout_marginEnd="10dp"
            android:background="?android:attr/listDivider" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="630dp"
            android:background="@drawable/button_style6"
            app:srcCompat="@drawable/drawable_logout"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="123dp"
            android:layout_marginTop="640dp"
            android:text="Logout"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="350dp"
            android:layout_marginTop="640dp"
            android:layout_marginEnd="25dp"
            app:srcCompat="@drawable/drawable_arrowright"
            tools:ignore="ImageContrastCheck" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
